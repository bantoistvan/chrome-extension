{
    "name": "My Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [ "contextMenus" ],
    "content_scripts": [
      {
        "matches": [ "*://*.youtube.com/*" ],
        "js": [ "main.js" ]
      }
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }